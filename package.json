{
  "name": "@featherweight/actions-reducer",
  "description": "Minimalistic typesafe actions and reducers",
  "version": "0.4.0",
  "keywords": [
    "action-creator",
    "action",
    "actions",
    "flux",
    "reducer",
    "redux-actions",
    "redux",
    "typescript"
  ],
  "main": "dist/index.js",
  "module": "dist/actions-reducer.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdx build",
    "prepublishOnly": "npm run build",
    "pub": "npm publish --public",
    "start": "tsdx watch",
    "test": "tsdx test"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm test"
    }
  },
  "devDependencies": {
    "@types/jest": "24.0.18",
    "husky": "4.0.0-beta.2",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1",
    "tsdx": "0.9.2",
    "tslib": "1.10.0",
    "typescript": "3.6.3"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "ignoreCodes": [
            "TS4104"
          ]
        }
      }
    }
  },
  "license": "MIT"
}
